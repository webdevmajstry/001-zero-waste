{
  "compilerOptions": {
    "target": "es5",
    "lib": ["es5", "dom"],
    "types": ["cypress", "node", "@testing-library/cypress"],
    "baseUrl": "..",
    "paths": {
      "@pages/*": ["src/pages/*"],
      "@views/*": ["src/views/*"],
      "@features/*": ["src/features/*"],
      "@components/*": ["src/components/*"],
      "@third-party": ["src/third-party/index.ts"],
      "@mocks/*": ["src/__mocks__/*"]
    },
  },
  "include": ["../node_modules/cypress", "../node_modules/@testing-lirbary/cypress", "**/*.ts"]
}
